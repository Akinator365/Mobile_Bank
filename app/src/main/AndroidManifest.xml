<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilebank">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileBank">
        <activity
            android:name=".ui.dashboard.Balance"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Payhistory"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.CardTopUp"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Bankcard"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Cardinfo"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Cardfee"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Schoolfare"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Electricity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Waterfee"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.Pay"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>